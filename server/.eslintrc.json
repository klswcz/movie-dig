{
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prefer-const": "error",
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "trailingComma": "none",
          "printWidth": 120,
          "tabWidth": 4
        }
      ],
        "no-console": 2
    }
  }