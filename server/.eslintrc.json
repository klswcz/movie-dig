{
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "trailingComma": "none",
          "printWidth": 120,
          "tabWidth": 4
        }
      ],
        "no-console": 2
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    }
  }